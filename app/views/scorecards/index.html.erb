<h1>Scorecards</h1>



<% @scorecards.each do |card| %>
<div class="scorecards" data-id="<%= card.id %>">"
<tr>
	<td><%= link_to 'Course Played', scorecard_path(card) %></td>
	<td><%= link_to 'New Scorecard', new_scorecard_path %></td>
	<td><%= link_to 'Edit Scorecard', edit_scorecard_path(card) %></td>
	<td><%= link_to 'Destroy', method: :delete %></td>
</tr>

	<% end %>

	<br>